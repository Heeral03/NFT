pragma solidity ^0.8.18;
import {ERC721} from "../../lib/openzeppelin-contracts/contracts/token/ERC721/ERC721.sol";
import {Base64} from "../../lib/openzeppelin-contracts/contracts/utils/Base64.sol";
import {Test,console} from "../../lib/forge-std/src/Test.sol";
import {MoodNFT} from "../../src/MoodNFT.sol";
import {DeployMoodNFT} from "../../script/DeployMoodNFT.s.sol";
contract MoodNftIntegrations is Test{
    DeployMoodNFT deployer;
    MoodNFT moodNFT;
    string public constant HAPPY_SVG_URI= 
    "data:image/svg+xml;base64,";
    string public constant SAD_SVG_URI =
     "data:image/svg+xml;base64,";

    address USER=makeAddr("USER");
    function setUp() public{

        deployer=new DeployMoodNFT();
        moodNFT=deployer.run();

    }

    function testViewTokenURIIntegration() public {
        vm.prank(USER);
        moodNFT.mintNft();
        console.log(moodNFT.tokenURI(0));
    }
    function testFlipTokenToSad() public{
        vm.prank(USER);
        moodNFT.mintNft();

        vm.prank(USER);
        moodNFT.flipMood(0);

        string memory sadSvgUri=
        "data:application/json;base64,";
        //moodNFT.tokenURI(0), which returns a JSON metadata URI 
        assertEq(keccak256(abi.encodePacked(moodNFT.tokenURI(0))),keccak256(abi.encodePacked(sadSvgUri)));
    }
}